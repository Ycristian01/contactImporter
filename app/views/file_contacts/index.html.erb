<h1>List of imported files</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Status</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
  <% @file_contacts.each do |file| %>
    <tr>
      <th scope="row"><%= file.id %></th>
      <td><%= file.name if file.name%></td>
      <td><%= file.status if file.status %></td>
      <td><%=link_to "import", import_file_contacts_path(id: file), method: :post%></td>
    </tr>
  <% end %>
  </tbody>
</table>