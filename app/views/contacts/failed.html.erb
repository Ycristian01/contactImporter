<h1>List of failed files</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Errors</th>
      <th scope="col">File</th>
    </tr>
  </thead>
  <tbody>
  <% @failed_contacts.each do |contact| %>
    <tr>
      <th scope="row"><%= contacto.id %></th>
      <td><%= contact.name if contact.name%></td>
      <td><%= contact.contact_errors if contact.contact_errors %></td>
      <td><%= contact.file_contact.name if contact.file_contact %></td>
    </tr>
  <% end %>
  </tbody>
</table>